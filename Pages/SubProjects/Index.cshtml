@page
@model BizQuickTime.Web.Pages.SubProjects.IndexModel

@{
    ViewData["Title"] = "Sub Projects";
    
    ViewData["ActivePage"] = BizQuickTime.Web.Pages.Shared.ManageNavPages.Subprojects;
}

    

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SubProject[0].SubProjectID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SubProject[0].Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SubProject[0].CalcType1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SubProject[0].CalcType2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SubProject[0].CalcType3)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.SubProject) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SubProjectID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CalcType1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CalcType2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CalcType3)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.ID">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
